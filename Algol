        glowTween:Play()
    else
        scaffoldGlow.BackgroundColor3 = Color3.fromRGB(100, 0, 200)
        TweenService:Create(scaffoldGlow, TweenInfo.new(0.3), {BackgroundTransparency = 0.8}):Play()
