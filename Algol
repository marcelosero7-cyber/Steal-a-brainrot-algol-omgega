local function makeBtn(text, ypos)
    local btnFrame = Instance.new("Frame", Frame)
    btnFrame.Size = UDim2.new(0.85, 0, 0, 38)
    btnFrame.Position = UDim2.new(0.075, 0, 0, ypos)
    btnFrame.BackgroundTransparency = 1

    local glow = Instance.new("Frame", btnFrame)
    glow.Size = UDim2.new(1, 6, 1, 6)
    glow.Position = UDim2.new(0, -3, 0, -3)
    glow.BackgroundColor3 = Color3.fromRGB(100, 0, 200)
    glow.BackgroundTransparency = 0.8
    glow.ZIndex = btnFrame.ZIndex - 1
    
    local glowCorner = Instance.new("UICorner", glow)
    glowCorner.CornerRadius = UDim.new(0, 12)

    local b = Instance.new("TextButton", btnFrame)
    b.Size = UDim2.new(1, 0, 1, 0)
    b.BackgroundColor3 = Color3.fromRGB(40, 0, 80)
    b.TextColor3 = Color3.fromRGB(255, 255, 255)
    b.Font = Enum.Font.GothamBold
    b.TextSize = 16
    b.Text = text
    b.ZIndex = 10

    local btnCorner = Instance.new("UICorner", b)
    btnCorner.CornerRadius = UDim.new(0, 10)

    local btnGradient = Instance.new("UIGradient", b)
    btnGradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(60, 0, 120)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(40, 0, 80))
    }
    btnGradient.Rotation = 90

    b.MouseEnter:Connect(function()
        local hoverTween = TweenService:Create(glow, TweenInfo.new(0.2), {BackgroundTransparency = 0.5})
        hoverTween:Play()
        local scaleTween = TweenService:Create(btnFrame, TweenInfo.new(0.2), {Size = UDim2.new(0.88, 0, 0, 40)})
        scaleTween:Play()
    end)

    b.MouseLeave:Connect(function()
        local hoverTween = TweenService:Create(glow, TweenInfo.new(0.2), {BackgroundTransparency = 0.8})
        hoverTween:Play()
        local scaleTween = TweenService:Create(btnFrame, TweenInfo.new(0.2), {Size = UDim2.new(0.85, 0, 0, 38)})
        scaleTween:Play()
    end)

    return b, glow
end

local SpeedBtn, speedGlow = makeBtn("Speed: OFF", 65)
local GravBtn, gravGlow = makeBtn("Gravity: OFF", 115)
local EspBtn, espGlow = makeBtn("Player ESP: OFF", 165)

local function createParticle()
    local particle = Instance.new("Frame", Frame)
    particle.Size = UDim2.new(0, 2, 0, 2)
    particle.Position = UDim2.new(math.random(), 0, math.random(), 0)
    particle.BackgroundColor3 = Color3.fromRGB(255, math.random(100, 255), 255)
    particle.BorderSizePixel = 0
    particle.ZIndex = 1

    local particleCorner = Instance.new("UICorner", particle)
    particleCorner.CornerRadius = UDim.new(0, 1)

    local moveTween = TweenService:Create(particle, TweenInfo.new(3), {
        Position = UDim2.new(math.random(), 0, 1.2, 0),
        BackgroundTransparency = 1
    })
    moveTween:Play()

    moveTween.Completed:Connect(function()
        particle:Destroy()
    end)
end

spawn(function()
    while Frame.Parent do
        createParticle()
        wait(0.5)
    end
end)

RunService.RenderStepped:Connect(function()
    if speedEnabled and LocalPlayer.Character then
        local char = LocalPlayer.Character
        local hrp = char:FindFirstChild("HumanoidRootPart")
        local humanoid = char:FindFirstChild("Humanoid")
        if hrp and humanoid then
            local moveDir = humanoid.MoveDirection
            if moveDir.Magnitude > 0 then
                hrp.Velocity = Vector3.new(moveDir.Unit.X * speed, hrp.Velocity.Y, moveDir.Unit.Z * speed)
            end
        end
    end
end)

SpeedBtn.MouseButton1Click:Connect(function()
    speedEnabled = not speedEnabled
    SpeedBtn.Text = speedEnabled and "Speed: ON" or "Speed: OFF"
    
    if speedEnabled then
        speedGlow.BackgroundColor3 = Color3.fromRGB(0, 255, 128)
        local glowTween = TweenService:Create(speedGlow, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), {BackgroundTransparency = 0.3})
        glowTween:Play()
    else
        speedGlow.BackgroundColor3 = Color3.fromRGB(100, 0, 200)
        TweenService:Create(speedGlow, TweenInfo.new(0.3), {BackgroundTransparency = 0.8}):Play()
    end
end)

local function applyGravity(char)
    local hrp = char:WaitForChild("HumanoidRootPart")
    local hum = char:WaitForChild("Humanoid")
    local mass = hrp.AssemblyMass

    hum.UseJumpPower = true  
    hum.JumpPower = customJumpPower  

    local att = hrp:FindFirstChild("CustomGravityAttachment")  
    if not att then  
        att = Instance.new("Attachment")  
        att.Name = "CustomGravityAttachment"  
        att.Parent = hrp  
    end  

    local vf = hrp:FindFirstChild("CustomGravityForce")  
    if not vf then  
        vf = Instance.new("VectorForce")  
        vf.Name = "CustomGravityForce"  
        vf.Attachment0 = att  
        vf.RelativeTo = Enum.ActuatorRelativeTo.World  
        vf.Parent = hrp  
    end  

    RunService.Heartbeat:Connect(function()  
        if hrp and hrp.Parent and gravityEnabled then  
            local difference = 196.2 - customGravity  
            vf.Force = Vector3.new(0, mass * difference, 0)  
        elseif vf then  
            vf.Force = Vector3.new(0,0,0)  
        end  
    end)
end

GravBtn.MouseButton1Click:Connect(function()
    gravityEnabled = not gravityEnabled
    GravBtn.Text = gravityEnabled and "Gravity: ON" or "Gravity: OFF"
    
    if gravityEnabled then
        gravGlow.BackgroundColor3 = Color3.fromRGB(255, 100, 0)
        local glowTween = TweenService:Create(gravGlow, TweenInfo.new(0.7, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), {BackgroundTransparency = 0.3})
        glowTween:Play()
        if LocalPlayer.Character then
            applyGravity(LocalPlayer.Character)
        end
    else
        gravGlow.BackgroundColor3 = Color3.fromRGB(100, 0, 200)
        TweenService:Create(gravGlow, TweenInfo.new(0.3), {BackgroundTransparency = 0.8}):Play()
    end
end)

LocalPlayer.CharacterAdded:Connect(function(char)
    if gravityEnabled then
        applyGravity(char)
    end
end)

local espObjects = {}

local function createESP(player)
    if espObjects[player] then return end
    local billboard = Instance.new("BillboardGui")
    billboard.Name = "ESP"
    billboard.Size = UDim2.new(0, 100, 0, 20)
    billboard.AlwaysOnTop = true

    local text = Instance.new("TextLabel", billboard)  
    text.Size = UDim2.new(1, 0, 1, 0)  
    text.BackgroundTransparency = 1  
    text.TextColor3 = Color3.fromRGB(180, 0, 255)  
    text.Font = Enum.Font.GothamBold  
    text.TextScaled = true  
    text.Text = player.Name  

    espObjects[player] = billboard  
    if player.Character and player.Character:FindFirstChild("Head") then  
        billboard.Adornee = player.Character.Head  
        billboard.Parent = player.Character.Head  
    end  

    player.CharacterAdded:Connect(function(char)  
        char:WaitForChild("Head")  
        billboard.Adornee = char.Head  
        billboard.Parent = char.Head  
    end)
end

local function toggleESP(state)
    espEnabled = state
    if espEnabled then
        for _, p in pairs(Players:GetPlayers()) do
            if p ~= LocalPlayer then
                createESP(p)
            end
        end
        Players.PlayerAdded:Connect(function(p)
            if espEnabled and p ~= LocalPlayer then
                createESP(p)
            end
        end)
    else
        for _, obj in pairs(espObjects) do
            obj:Destroy()
        end
        espObjects = {}
    end
end

EspBtn.MouseButton1Click:Connect(function()
    espEnabled = not espEnabled
    EspBtn.Text = espEnabled and "Player ESP: ON" or "Player ESP: OFF"
    
    if espEnabled then
        espGlow.BackgroundColor3 = Color3.fromRGB(255, 0, 255)
        local glowTween = TweenService:Create(espGlow, TweenInfo.new(0.4, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), {BackgroundTransparency = 0.3})
        glowTween:Play()
    else
        espGlow.BackgroundColor3 = Color3.fromRGB(100, 0, 200)
        TweenService:Create(espGlow, TweenInfo.new(0.3), {BackgroundTransparency = 0.8}):Play()
    end
    
    toggleESP(espEnabled)
end)
